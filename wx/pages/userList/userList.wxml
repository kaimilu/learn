<view>
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="width:100%; height:400rpx">
        <block wx:for="{{imgUrls}}">
            <swiper-item>
                <image src="{{item}}" class="slide-image" />
            </swiper-item>
        </block>
    </swiper>

    <view class="user-container">
        
        <view class="user-list" wx:for="{{userList}}">
            <image src="/images/logo.jpg"></image>
            <view class="user-info">
                <view class="item">
                    <text>{{userName}}</text>
                    <text>{{juli}}</text>
                </view>
                <view class="item">
                    <text>{{live}}</text>
                    <text class="pink">{{yz}}</text>
                    <text class="oringe">{{js}}</text>
                </view>
                <view class="item">
                    <text>{{desc}}</text>
                </view>
            </view>
        </view>
     
        
    </view>
</view>
